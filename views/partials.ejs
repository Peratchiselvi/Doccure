            <li class="nav-item dropdown has-arrow logged-item">
                <a href="#" class="dropdown-toggle nav-link" data-toggle="dropdown">
                    <span class="user-img">
                        <% if(user.img){ %>
                            <img class="rounded-circle"
                                src="data:doctor/<%=user.img.contentType%>;base64,<%=user.img.data.toString('base64')%>"
                                width="31" alt="Ryan Taylor">
                            <% } else{ %>
                                <img class="rounded-circle" src="img/doctors/doctor-thumb-02.jpg" width="31">
                                <% } %>
                    </span>
                </a>
